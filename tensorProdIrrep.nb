Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"BeginPackage", "[", "\"\<shorttensorProdIrrepPackage`\>\"", "]"}],
    ";"}], "\n", 
  RowBox[{"(*", 
   RowBox[{"usages", " ", "go", " ", "here"}], "*)"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"oblade", "::", "usage"}], "=", "\n", 
    "\"\<Evaluation of oblade[n] initializes all relevant data for SU(n). It \
returns n and set the variable currentgroup to n.\>\""}], ";"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"tensorproduct", "::", "usage"}], "=", 
    "\"\<tensorproduct[lambda,mu] returns the decomposition into irreducible \
components nu of a tensor product of two irreps lambda, mu, of the Lie group \
SU(n), given by the components of their highest weights along the basis of \
fundamental weights.  The last entry of each term is its multiplicity.\>\""}],
    ";"}], "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"tenspromatform", "::", "usage"}], "=", "\[IndentingNewLine]", 
    "\"\<tenspromatform[{lambda,mu}] returns tensorproduct[lambda,mu]\>\""}], 
   ";"}], "\n"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"currentgroup", "::", "usage"}], " ", "=", " ", 
    "\"\<currentgroup returns the argument n of the group SU(n) currently in \
use.\>\""}], ";"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"Begin", "[", "\"\<Private`\>\"", "]"}], ";"}], "\n", "\n", 
  RowBox[{"(*", " ", 
   RowBox[{"Sides", ",", "edges", ",", 
    RowBox[{"components", " ", "and", " ", "equations"}]}], " ", "*)"}], "\n", 
  RowBox[{"(*", " ", "Sides", " ", "*)"}], "\n", 
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
    "p", " ", "will", " ", "run", " ", "from", " ", "1", " ", "to", " ", 
     "NN"}], "-", "1"}], " ", "*)"}]}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"lambdalabel", "[", 
    RowBox[{"n_", ",", "p_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"1", "/", "2"}], " ", 
    RowBox[{"(", 
     RowBox[{"2", "+", 
      RowBox[{"3", " ", "p"}], "+", 
      RowBox[{"p", "^", "2"}]}], ")"}]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"mulabel", "[", 
    RowBox[{"n_", ",", "p_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"n", "+", "1"}], ")"}], 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"n", "+", "2"}], ")"}], "/", "2"}]}], "-", "p"}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"nulabel", "[", 
     RowBox[{"n_", ",", "p_"}], "]"}], ":=", 
    RowBox[{
     RowBox[{"1", "/", "2"}], " ", 
     RowBox[{"(", 
      RowBox[{"n", "+", 
       RowBox[{"n", "^", "2"}], "-", 
       RowBox[{"2", " ", "n", " ", "p"}], "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", "1"}], "+", "p"}], ")"}], " ", "p"}], "+", "2"}], 
      ")"}]}]}], ";"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"oblade", "[", "var_", "]"}], ":=", " ", 
   RowBox[{"With", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"NN", "=", "var"}], "}"}], ",", "\n", " ", "\n", 
     RowBox[{"(*", " ", 
      RowBox[{"Equations", " ", "on", " ", "sides"}], " ", "*)"}], "\n", 
     RowBox[{"(*", " ", 
      RowBox[{
      "conservation", " ", "equations", " ", "at", " ", "the", " ", 
       "external", " ", "vertices", " ", 
       RowBox[{"(", "weights", ")"}]}], " ", "*)"}], "\n", 
     RowBox[{
      RowBox[{"lambdaequations", " ", "=", " ", 
       RowBox[{"And", "[", "\n", 
        RowBox[{
         RowBox[{"Apply", "[", 
          RowBox[{"And", ",", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"lambda", "[", 
               RowBox[{"p", ",", "0", ",", "0"}], "]"}], "==", " ", 
              RowBox[{"Sum", "[", " ", 
               RowBox[{
                RowBox[{"j", "[", 
                 RowBox[{"p", ",", 
                  RowBox[{"NN", "-", 
                   RowBox[{"(", 
                    RowBox[{"s", "+", "p"}], ")"}]}], ",", "s"}], "]"}], " ", 
                ",", 
                RowBox[{"{", 
                 RowBox[{"s", ",", "0", ",", 
                  RowBox[{"NN", "-", "p"}]}], "}"}]}], "]"}]}], ",", 
             RowBox[{"{", 
              RowBox[{"p", ",", "1", ",", 
               RowBox[{"NN", "-", "1"}]}], "}"}]}], "]"}]}], "]"}], ",", "\n", 
         RowBox[{"Apply", "[", 
          RowBox[{"And", ",", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"lambda", "[", 
               RowBox[{"0", ",", "p", ",", "0"}], "]"}], "==", " ", 
              RowBox[{"Sum", "[", " ", 
               RowBox[{
                RowBox[{"j", "[", 
                 RowBox[{"s", ",", "p", ",", 
                  RowBox[{"NN", "-", 
                   RowBox[{"(", 
                    RowBox[{"s", "+", "p"}], ")"}]}]}], "]"}], " ", ",", 
                RowBox[{"{", 
                 RowBox[{"s", ",", "0", ",", 
                  RowBox[{"NN", "-", "p"}]}], "}"}]}], "]"}]}], ",", 
             RowBox[{"{", 
              RowBox[{"p", ",", "1", ",", 
               RowBox[{"NN", "-", "1"}]}], "}"}]}], "]"}]}], "]"}], ",", "\n", 
         RowBox[{"Apply", "[", 
          RowBox[{"And", ",", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"lambda", "[", 
               RowBox[{"0", ",", "0", ",", "p"}], "]"}], "==", " ", 
              RowBox[{"Sum", "[", " ", 
               RowBox[{
                RowBox[{"j", "[", 
                 RowBox[{"s", ",", 
                  RowBox[{"NN", "-", 
                   RowBox[{"(", 
                    RowBox[{"s", "+", "p"}], ")"}]}], ",", "p"}], "]"}], " ", 
                ",", 
                RowBox[{"{", 
                 RowBox[{"s", ",", "0", ",", 
                  RowBox[{"NN", "-", "p"}]}], "}"}]}], "]"}]}], ",", 
             RowBox[{"{", 
              RowBox[{"p", ",", "1", ",", 
               RowBox[{"NN", "-", "1"}]}], "}"}]}], "]"}]}], "]"}]}], "]"}]}],
       ";", "\n", 
      RowBox[{"(*", " ", 
       RowBox[{"Edges", " ", "values", " ", 
        RowBox[{"(", 
         RowBox[{
         "in", " ", "terms", " ", "of", " ", "fundamental", " ", 
          "pictographs"}], ")"}]}], "*)"}], "\n", 
      RowBox[{
       RowBox[{"edgesEW", "[", "NN", "]"}], "=", " ", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"FoldList", "[", "Plus", "]"}], "[", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"j", "[", 
             RowBox[{"t", ",", 
              RowBox[{"NN", "-", "s"}], ",", 
              RowBox[{"s", "-", "t"}]}], "]"}], ",", 
            RowBox[{"{", 
             RowBox[{"s", ",", "t", ",", 
              RowBox[{"NN", "-", "1"}]}], "}"}]}], "]"}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"t", ",", "1", ",", 
           RowBox[{"NN", "-", "1"}]}], "}"}]}], "]"}]}], ";", "\n", 
      RowBox[{
       RowBox[{"edgesNESW", "[", "NN", "]"}], "=", " ", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"FoldList", "[", "Plus", "]"}], "[", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"j", "[", 
             RowBox[{
              RowBox[{"s", "-", "p"}], ",", "p", ",", 
              RowBox[{"NN", "-", "s"}]}], "]"}], ",", 
            RowBox[{"{", 
             RowBox[{"s", ",", "p", ",", 
              RowBox[{"NN", "-", "1"}]}], "}"}]}], "]"}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"p", ",", "1", ",", 
           RowBox[{"NN", "-", "1"}]}], "}"}]}], "]"}]}], ";", "\n", 
      RowBox[{
       RowBox[{"edgesNWSE", "[", "NN", "]"}], "=", " ", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"FoldList", "[", "Plus", "]"}], "[", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"j", "[", 
             RowBox[{
              RowBox[{"NN", "-", "s"}], ",", 
              RowBox[{"s", "-", "p"}], ",", "p"}], "]"}], ",", 
            RowBox[{"{", 
             RowBox[{"s", ",", "p", ",", 
              RowBox[{"NN", "-", "1"}]}], "}"}]}], "]"}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"p", ",", "1", ",", 
           RowBox[{"NN", "-", "1"}]}], "}"}]}], "]"}]}], ";", "\n", 
      RowBox[{"(*", " ", 
       RowBox[{"Edge", " ", "labelling", " ", "doublets"}], " ", "*)"}], "\n", 
      RowBox[{
       RowBox[{"edgesEWlabels", "[", "lev_", "]"}], ":=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"Take", "[", 
          RowBox[{
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"1", "/", "2"}], " ", 
               RowBox[{"(", 
                RowBox[{"4", "+", "p", "+", 
                 RowBox[{"p", "^", "2"}]}], ")"}]}], "+", 
              RowBox[{"(", 
               RowBox[{"lev", "-", "1"}], ")"}]}], ",", 
             RowBox[{"{", 
              RowBox[{"p", ",", "lev", ",", "NN"}], "}"}]}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"z", ",", 
             RowBox[{"z", "+", "1"}]}], "}"}]}], "]"}], ",", 
